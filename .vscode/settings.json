{
  "fiveServer.highlight": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnType": true,
  "notebook.formatOnCellExecution": true,
  "notebook.formatOnSave.enabled": true,
  "prettier.experimentalTernaries": true,
  "json.format.keepLines": true,
  "files.autoSave": "afterDelay",
  "diffEditor.codeLens": true,
  "diffEditor.wordWrap": "off",
  "typescript.check.npmIsInstalled": false,
  "typescript.validate.enable": false,
  "typescript.workspaceSymbols.excludeLibrarySymbols": false,
  "typescript.tsserver.web.projectWideIntellisense.enabled": false,
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "breadcrumbs.showNull": true,
  "outline.showNull": true,
  "editor.defaultColorDecorators": "never",
  "workbench.preferredLightColorTheme": "Default Light Modern",
  "window.autoDetectColorScheme": true,
  "errorLens.statusBarColorsEnabled": false,
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.preferredDarkColorTheme": "Default Dark Modern",
  "workbench.preferredHighContrastColorTheme": "Default Dark Modern",
  "workbench.iconTheme": "vs-seti"
}
